{
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": ["eslint:recommended", "plugin:react/recommended"],
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "rules": {
      "no-console": "off",
      "react/react-in-jsx-scope": "off",
      "max-len": ["error", { "code": 100 }]
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  }
  